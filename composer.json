{
    "name": "src-run/arthur-doctrine-identity-library",
    "license": "MIT",
    "type": "library",
    "homepage": "https://src.run/arthur-doctrine-identity-library",
    "description": "base identity entity classes (id- slug- uuid-based, etc)",
    "support": {
        "email": "rmf@src.run",
        "issues": "https://src.run/arthur-doctrine-identity-library/issues",
        "source": "https://src.run/arthur-doctrine-identity-library/git"
    },
    "keywords": [
        "arthur-doctrine-identity-library", "arthur", "doctrine", "exception", "library", "rmf", "src-run"
    ],
    "authors": [
        {
            "name": "Rob Frawley 2nd",
            "email": "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/common": "^2.5",
        "ramsey/uuid": "^3.0",
        "src-run/augustus-reflection-library": "~0.7",
        "src-run/augustus-serializer-library": "~0.7",
        "src-run/arthur-doctrine-exception-library": "~0.8"
    },
    "require-dev": {
        "codacy/coverage": "^1.0",
        "friendsofphp/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "^5.5",
        "satooshi/php-coveralls": "^1.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1",
        "symfony/phpunit-bridge": "^3.0"
    },
    "suggest": {
        "doctrine/orm": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "SR\\Doctrine\\ORM\\Mapping\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SR\\Doctrine\\ORM\\Mapping\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
